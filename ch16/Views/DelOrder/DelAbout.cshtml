    ViewBag.Title = "DelAbout";
}
<h2><img src="~/images/identity.png" width="50" /><label color: #fff;> 個人資料</label></h2>
<div class="row">
    <div class="col-xs-10">
        <table class="table table-hover" >            
            <tr>
                <th>
                    帳號:
                </th>
                <th>
                    @Model.duserid
                </th>
            </tr>
            <tr>
                <th>
                    姓名:
                </th>
                <th>
                    @Model.dname
                </th>
            </tr>
            <tr>
                <th>
                    電話:
                </th>
                <th>
                    @Model.dphone
                </th>
            </tr>
            <tr>
                <td></td>
                <td>@Html.ActionLink("修改資料", "Edit", new { id = Model.duserid })</td>
            </tr>
        </table>
    </div>
</div>
